{
  "name": "ustyle-react",
  "version": "0.0.7",
  "description": "uStyle React Components",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist ; babel src --out-dir dist",
    "start": "webpack-dev-server",
    "commit-dist": "npm run build ; git add dist",
    "bump-version": "nver; git add package.json",
    "watch": "nodemon -e js --watch src --exec \"npm run build\""
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uswitch/ustyle-react.git"
  },
  "author": "uSwitch Limited",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/uswitch/ustyle-react/issues"
  },
  "homepage": "https://github.com/uswitch/ustyle-react#readme",
  "pre-commit": [
    "bump-version",
    "commit-dist"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "nver": "^0.0.15",
    "prop-types": "^15.3",
    "react": ">=15.3"
  },
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "nodemon": "^1.11.0",
    "pre-commit": "^1.2.2",
    "react-dom": ">=15.3",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  }
}